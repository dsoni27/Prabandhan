<UserControl x:Class="Parbandhan.UserControls.PasswordUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Parbandhan.UserControls"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="300">
    <Grid>
        <PasswordBox Name="PasswordBox" Background="Transparent" CaretBrush="LightGray" BorderThickness="0,0,0,0" Foreground="White" PasswordChanged="PasswordBox_PasswordChanged"/>
    </Grid>
</UserControl>
